
<!-- My form -->
<%= form_for [@my_project, @my_entry] do |f| %>

	<%= f.label :hours %>
	<%= f.number_field :hours %>

	</br>

	<%= f.label :minutes %>
	<%= f.number_field :minutes %>

	</br>


	<%= f.label :comments %>
	<%= f.text_field :comments %>

	</br>


	<%= f.label :date %>
	<%= f.date_field :date %>

	</br>

	<%= f.button %>
	<%= link_to 'Cancel', project_time_entries_path(@my_project) %>

<% end %>
