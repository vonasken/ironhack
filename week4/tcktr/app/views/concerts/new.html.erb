<h1> New Concert</h1>

<%= form_for :concert, url: concerts_path do |c| %>
		<p> 
			<%= c.label :artist %><br>
			<%= c.text_field :artist %>
		</p>


		<p> 
			<%= c.label :venue %><br>
			<%= c.text_field :venue %>
		</p>

		<p> 
			<%= c.label :date %><br>
			<%= c.date_field :date %>
		</p>

		<p> 
			<%= c.label :price %><br>
			<%= c.number_field :price %>
		</p>

		<p> 
			<%= c.label :description %><br>
			<%= c.text_field :description %>
		</p>

		<p>
		<%= c.submit %>
		</p>
<% end %>

			
